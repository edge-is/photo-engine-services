redis:
  server: '127.0.0.1'
  port: 6379
index:
  elasticsearch:
   - host: 'localhost'
log:
  worker:
    error: 'worker-error.log'
    info: 'worker-error.log'
  watch:
    error: 'watch-error.log'
    info: 'watch-error.log'
status:
  channel: 'sync-test-worker-status'
api:
  port: 9000
worker:
  timeout: 20000
interval: '3h'
queue: 'QueName'
include: './conf.d'
